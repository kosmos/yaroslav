include ../preview/preview
include ../nav-menu/nav-menu
include ../menu/menu

mixin b_header(data)
  +b_preview(preview)
  +b("header").header.row.expanded
    +b_nav-menu(data.share)
    +b.over.small-5.small-offset-6.end
      +b.caption
        +e.big!= data.big
        +e.middle!= data.middle
    +b_menu(menu)
