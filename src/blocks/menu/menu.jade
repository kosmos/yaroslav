mixin menu()
  +b.NAV.navigation.expanded.column.row
    +e.block.small-6.columns
      +b.UL.navigation-list
        each val, index in menu
          - var num = index+1
          +e.LI.item
            +e.SPAN.value= val
            +e.SPAN.dots
            +e.SPAN.number= "0" + num
    +e.block.small-6.columns
      +b.thumbnail= "cercrrtr"
    +b.SPAN.close
